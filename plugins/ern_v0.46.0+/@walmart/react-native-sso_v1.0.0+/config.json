{
  "android": {
    "root": "android",
    "dependencies": [
      "api 'com.google.dagger:dagger-android:2.30.1'",
      "api 'com.google.dagger:dagger-android-support:2.30.1'",
      "annotationProcessor 'com.google.dagger:dagger-android-processor:2.30.1'",
      "annotationProcessor 'com.google.dagger:dagger-compiler:2.30.1'",
      "api 'com.walmart.ssmp.sso:wm-sso-client:1.2.0'",
      "compileOnly 'com.walmart.ssmp:platform-core:1.0.3'",
      "implementation 'net.openid:appauth:0.7.1'"
    ],
    "repositories": [
      "maven { url 'https://repository.walmart.com/content/groups/public/' }"
    ]
  },
  "ios": {
    "extraPodspecsSources": ["https://gecgithub01.walmart.com/store-systems-associate-tech-platform/cocoapods-specs.git"],
    "extraPodfileStatements": ["use_frameworks! :linkage => :static"]
  }
}
